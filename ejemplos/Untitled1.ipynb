    "from flask import Flask\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/\")\n",
    "@app.route(\"/saluda\")\n",
    "def hola():\n",
    "    return \"Hola\"\n",
    "\n",
    "@app.route(\"/login\")\n",
    "@app.route(\"/login/<name>\")\n",
    "def login(name=\"usuario\"):\n",
    "    return \"Login {}\".format(name)\n",
    "\n",
    "app.run()"
   ]
  }
